translate spanish mary_wollstonecraft_124177df:

    # mary_wollstonecraft "Què dius? Ja? Sí que passa el temps ràpid ..."
    mary_wollstonecraft "¿Qué dices? ¿Ya? Sí que pasa rápido el tiempo ..."

# game/screens/chapter0/all.rpy:226
translate spanish mary_wollstonecraft_fd789082:

